<% if(typeof newData != 'undefined'){%>
<div class="newdata">
  <% if(typeof newData.title != 'undefined'){%>
  <h2><%= newData.title; %></h2>
  <% } %>
  <form method="POST" action="<%= newData.action; %>">
    <% newData.inputs.forEach( input => {%>
    <div>
      <label><%= input.label; %></label>
      <input
        class="w3-input w3-border w3-round"
        type="text"
        name="<%= input.name; %>"
        value="<%= input.value; %>"
      />
    </div>
    <% }); %>
    <button type="sumbit"><%= newData.button; %></button>
  </form>
</div>
<% } %>
